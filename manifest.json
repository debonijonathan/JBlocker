{
    "name": "JonnyADBlocker", 
    "description": "Experience a cleaner, faster web and block annoying ads",
    "manifest_version": 2,
    "version": "1.0", 
    "background": {
        "scripts": ["list_blocked.js", "JonnyBlocker.js"]
    },
    "permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/"],
    "browser_action":{
        "default_icon":"icon.png",
        "default_title":"JonnyBlocker - The cleaner Ad Blocker"
    }
}